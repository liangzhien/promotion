.right select {
	line-height: 1;
}
.arrow {
	position: absolute;
	top: 50%;
	right: 2px;
	-webkit-transform: translate(0, -50%);
	transform: translate(0, -50%);
}



.page2_apply #button2 {
	display: block;
	position: absolute; top: 50%; left: 50%;
	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	margin: 0px;
	text-align: center;
	font-size: 16px;
	font-weight: bold;
	background: rgba(190, 0, 0, 1) -moz-linear-gradient(left, rgba(197, 0, 0, 1), rgba(145, 0, 0, 1));;
	background: rgba(190, 0, 0, 1) -webkit-gradient(linear, left center, right center, color-stop(0, rgba(197, 0, 0, 1)), color-stop(1, rgba(145, 0, 0, 1)));
	border: none;
	padding: 3px 0;
	width: 40%;
	color: #fff;
    border-radius: 0;
	-webkit-text-shadow: 2px 3px 2px #000;
	text-shadow: 2px 3px 2px #000;
	-webkit-box-shadow: 3px 3px 3px rgba(0, 0, 0, 1);
	box-shadow: 3px 3px 3px rgba(0, 0, 0, 1);
}
.page2_time {
	text-align: center;
	overflow: hidden;
	font-size: 11px;
}
.page2_note {
	background-color: #fff;
	font-size: 12px;
	border: 2px solid rgba(197, 39, 45, 1);
	overflow: hidden;
	margin: 0 2px;
	text-align: justify;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.page2_note h3 {
	margin: 0;
	font-size: 12px;
	text-align: center;
}
.page2_note ul {
	padding: 0;
	margin: 0;
}
.page2_note li {
	padding-left: 1em;
	position: relative;
	padding: 0;
	margin-left: 1em;
	list-style-type: none;
}
.page2_note li:before {
	position: absolute;
	top: 0;
	right: 100%;
	z-index: 5;
}
.page2_note li:nth-of-type(1):before {
	content: '1)';
}
.page2_note li:nth-of-type(2):before {
	content: '2)';
}
.page2_note li:nth-of-type(3):before {
	content: '3)';
}
.page2_note li:nth-of-type(4):before {
	content: '4)';
}
.page2_note strong {
	color: rgba(205, 78, 84, 1);
}
.page2_declare {
	font-size: 12px;
	overflow: hidden;
	color: rgba(160, 160, 160, 1);
	background-color: rgba(230, 230, 230, 1);
	padding: 1px 2px 0;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.page2_contact {	
	background-color: rgba(230, 230, 230, 1);
	margin: 0 2px;
}
.page2_contact .contactWrapper {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 100%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	border: 2px solid #000;
}
.contactWrapper > div, .contactWrapper > a {
	display: block;
	float: left;
	width: 50%;
	padding: 2px 0;
	text-align: center;
	font-size: 16px;
	font-weight: bold;
	text-shadow: none;
}
.contactWrapper > div {
	background-color: rgba(128, 128, 128, 1);
	color: #fff;
}
.phone img {
	vertical-align: middle;
	height: 16px;
}
.contactWrapper > a {
	-webkit-border-radius: 0px;
	border-radius: 0;
	box-shadow: none;
	text-decoration: none;
	color: #000;
}