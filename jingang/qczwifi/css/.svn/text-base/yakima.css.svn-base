* {
	margin: 0;
	border: 0;
	padding: 0;
	outline: 0;
}
body {
	font: 14px/20px '黑体', 宋体', '微软雅黑', 'arial','sans-serif';
	background-color: rgba(162, 162, 162, 1);
}
select, input, option {
	-webkit-appearance: none;
	appearance: none;
	background-color: rgba(230, 230, 230, 1);
}
.clearfix:after {
	content: '';
	display: block;
	clear: both;
	height: 0;
	visibility: hidden;
	opacity: 0;
}
.page {
	height: 100%;
	overflow: hidden;
	background-color: rgb(230, 230, 230);
	min-height: 480px;
}

/*第1页布局*/
.page1_topbg {position: relative; height: 62.87%; height: 62%;}
.page1_bar1, .page1_bar2 {position: relative; height: 0.81%;}
.page1_midbg {position: relative; height: 25.34%;}
.page1_footer {position: relative; height: 10.17%;}
.page1_topbg > img {
	height: 100%;
	width: 100%;
}
.page1_bar1, .page1_bar2 {
	background: #f00;
}
.page1_midbg > img {
	width: 100%;
	height: 100%;
}
.page1_footer {
	background: #e9e9e9;
}
.page1_footer > a {
	display: block;
	position: absolute;
	top: 50%;
	left: 50%;
	width: 96%;
	height: 60%;
	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-ms-transform: translate(-50%, -50%);
	-o-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	font-size: 16px;
	padding-top: 3px;
	line-height: 1.3;
	letter-spacing: .5em;
	-webkit-animation: shine 800ms infinite;
	animation: shine 800ms infinite;
	color: #fff;
	border-radius: 0;
	-webkit-text-shadow: 2px 3px 2px #000;
	text-shadow: 2px 3px 2px #000;
	-webkit-box-shadow: 3px 3px 3px rgba(0, 0, 0, 1);
	box-shadow: 3px 3px 3px rgba(0, 0, 0, 1);
	text-align: center;
	font-weight: bold;
	text-decoration: none;
	color: rgba(253, 255, 254, 1);
	background: rgba(190, 0, 0, 1) -moz-linear-gradient(left, rgba(197, 0, 0, 1), rgba(145, 0, 0, 1));;
	background: rgba(190, 0, 0, 1) -webkit-gradient(linear, left center, right center, color-stop(0, rgba(197, 0, 0, 1)), color-stop(1, rgba(145, 0, 0, 1)));
}
@-webkit-keyframes shine {
	0% {color: rgba(253, 255, 254, 1);}
	100% {color: rgba(253, 255, 254, .7);}
}
@keyframes shine {
	0% {color: rgba(253, 255, 254, 1);}
	100% {color: rgba(253, 255, 254, .7);}
}
.page1_footer a img {
	position: absolute;
	bottom: 0%;
	left: 49%;
	-webkit-transform: translate(-50%, 0%);
	transform: translate(-50%, 0%);
	height: 10px;
	-webkit-animation: move 800ms infinite;
	animation: move 800ms infinite;
}
@-webkit-keyframes move {
	0% {bottom: 0%;}
	100% {bottom: 30%;}
}
@keyframes move {
	0% {bottom: 0%;}
	100% {bottom: 30%;}
}


/*第2页布局*/
.page2_topbg {position: relative; height: 10.72%; height: 12%;}
.page2_form {position: relative; height: 37.4%; height: 32.25%;}
.page2_apply {position: relative; height: 6.78%; height: 6%;}
.page2_time {position: relative; height: 4.15%;}
.page2_note {position: relative; height: 29.27%;}
.page2_declare {position: relative; height: 7%;}
.page2_contact {position: relative; height: 7.83%; height: 8%;}
.page2_topbg {
	background: url(../img/bg2_top.png) no-repeat top center/100%;
}
.page2_form {
	padding: 0 2px;
	background-color: rgba(255, 255, 255, 0);
}
.page2_form > form {
	height: 100%;
}
.item {
	position: relative;
	height: 16.66%;
	font-size: 14px;
	overflow: hidden;
	/*-webkit-box-sizing: border-box;
	box-sizing: border-box;*/
	border-bottom: 1px solid #ccc;
	background-color: #fff;
	overflow: hidden;
}


.item > .inner {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 100%;
	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	height: auto;
	/*line-height: 1;*/
	overflow: hidden;
}
.item span {
	float: left;
	width: 25%;
	font-size: 14px;
}
.item .right {
	float: left;
	width: 75%;
	font-size: 12px;
	background-color: #fff;
}
.right input, .right select {
	width: 100%;
	background-color: #fff;
	font-size: 12px;
	line-height: normal;
	padding: 0;
}
input:-webkit-input-placeholder {
	line-height: normal;
}
input:input-placeholder {
	line-height: normal;
}
